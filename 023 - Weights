#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t; cin>>t;
	while(t--) {
	    int w,x,y,z; cin>>w>>x>>y>>z;
	    
	    int a=x+y+z,b=x+y,c=y+z,d=x+z;
	    if(w==x||w==y||w==z||w==a||w==b||w==c||w==d) {
	        cout<<"YES\n";
	    } else {
	        cout<<"NO\n";
	    }
	}
	return 0;
}
